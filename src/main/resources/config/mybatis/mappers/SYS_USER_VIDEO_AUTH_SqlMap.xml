            <if test="userId != null and userId != ''">#{userId, jdbcType=VARCHAR},</if>
            <if test="serialNo != null and serialNo != ''">#{serialNo, jdbcType=VARCHAR},</if>
            <if test="(isExist != null and isExist != '') or isExist == 0">#{isExist, jdbcType=INTEGER},</if>
