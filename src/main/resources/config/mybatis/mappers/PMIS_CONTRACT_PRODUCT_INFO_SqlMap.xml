                <if test="(htyhf != null and htyhf != '') or htyhf == 0">HTYHF = #{htyhf, jdbcType=BIGINT},</if>
                <if test="(cpxx != null and cpxx != '') or cpxx == 0">CPXX = #{cpxx, jdbcType=BIGINT},</if>
