        NULL AS STATUS,
        NULL AS MESSAGE,
        0 AS SOURCE_TYPE,
        0 AS CREATOR,
        GETDATE() AS CREATE_TIME,
        0 AS OPERATOR,
        NULL AS OPERATOR_TIME
        FROM
        PMIS_CONTRACT_PRODUCT_INFO t
        <where>
            <if test="(pmId != null ) or pmId == 0">and t.XMLCB = #{pmId,jdbcType=BIGINT}</if>
            <if test="(pmId != null ) or pmId == 0">and t.HTCPLB = 9</if>
